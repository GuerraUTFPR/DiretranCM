<p id="notice"><%= notice %></p>

<h1>Documentos</h1>

<table>
  <thead>
    <tr>
      <th>Rg</th>
      <th>Cpf</th>
      <th>Cid</th>
      <th>Foto</th>
      <th>Comp residencia</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @documentos.each do |documento| %>
      <tr>
        <td><%= documento.rg %></td>
        <td><%= documento.cpf %></td>
        <td><%= documento.cid %></td>
        <td><%= documento.foto %></td>
        <td><%= documento.comp_residencia %></td>
        <td><%= link_to 'Show', documento %></td>
        <td><%= link_to 'Edit', edit_documento_path(documento) %></td>
        <td><%= link_to 'Destroy', documento, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Documento', new_documento_path %>
