<%- model_class = Cadastro -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.pluralize.titleize %></h1>
</div>
<table class="table table-striped">
  <thead>
    <tr>
      <th><%= model_class.human_attribute_name(:id) %></th>
      <th><%= model_class.human_attribute_name(:nome) %></th>
      <th><%= model_class.human_attribute_name(:rg) %></th>
      <th><%= model_class.human_attribute_name(:cpf) %></th>
      <th><%= model_class.human_attribute_name(:data_nascimento) %></th>
      <th><%= model_class.human_attribute_name(:rua) %></th>
      <th><%= model_class.human_attribute_name(:numero) %></th>
      <th><%= model_class.human_attribute_name(:complemento) %></th>
      <th><%= model_class.human_attribute_name(:bairro) %></th>
      <th><%= model_class.human_attribute_name(:cidade) %></th>
      <th><%= model_class.human_attribute_name(:telefone) %></th>
      <th><%= model_class.human_attribute_name(:celular) %></th>
      <th><%= model_class.human_attribute_name(:usuarios_id) %></th>
      <th><%= model_class.human_attribute_name(:created_at) %></th>
      <th><%=t '.actions', :default => t("helpers.actions") %></th>
    </tr>
  </thead>
  <tbody>
    <% @cadastros.each do |cadastro| %>
      <tr>
        <td><%= link_to cadastro.id, cadastro_path(cadastro) %></td>
        <td><%= cadastro.nome %></td>
        <td><%= cadastro.rg %></td>
        <td><%= cadastro.cpf %></td>
        <td><%= cadastro.data_nascimento %></td>
        <td><%= cadastro.rua %></td>
        <td><%= cadastro.numero %></td>
        <td><%= cadastro.complemento %></td>
        <td><%= cadastro.bairro %></td>
        <td><%= cadastro.cidade %></td>
        <td><%= cadastro.telefone %></td>
        <td><%= cadastro.celular %></td>
        <td><%= cadastro.usuarios_id %></td>
        <td><%=l cadastro.created_at %></td>
        <td>
          <%= link_to t('.show', :default => t("helpers.links.show")),
                      cadastro_path(cadastro), :class => 'btn btn-default btn-xs' %>
          <%= link_to t('.edit', :default => t("helpers.links.edit")),
                      edit_cadastro_path(cadastro), :class => 'btn btn-default btn-xs' %>
          <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
                      cadastro_path(cadastro),
                      :method => :delete,
                      :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
                      :class => 'btn btn-xs btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to t('.new', :default => t("helpers.links.new")),
            new_cadastro_path,
            :class => 'btn btn-primary' %>
