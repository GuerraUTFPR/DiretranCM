<p id="notice"><%= notice %></p>

<h1>Inf Carteirinhas</h1>

<table>
  <thead>
    <tr>
      <th>Num registro</th>
      <th>Validade</th>
      <th>Diretor</th>
      <th>Nome beneficiario</th>
      <th>Rg</th>
      <th>Regras</th>
      <th>Tipo</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @inf_carteirinhas.each do |inf_carteirinha| %>
      <tr>
        <td><%= inf_carteirinha.num_registro %></td>
        <td><%= inf_carteirinha.validade %></td>
        <td><%= inf_carteirinha.diretor %></td>
        <td><%= inf_carteirinha.nome_beneficiario %></td>
        <td><%= inf_carteirinha.rg %></td>
        <td><%= inf_carteirinha.regras %></td>
        <td><%= inf_carteirinha.tipo %></td>
        <td><%= link_to 'Show', inf_carteirinha %></td>
        <td><%= link_to 'Edit', edit_inf_carteirinha_path(inf_carteirinha) %></td>
        <!-- <td><%= link_to 'Destroy', inf_carteirinha, method: :delete, data: { confirm: 'Are you sure?' } %></td> -->
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Inf Carteirinha', new_inf_carteirinha_path %>
