<%- model_class = Carteirinha -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.pluralize.titleize %></h1>
</div>
<table class="table table-striped">
  <thead>
    <tr>
      <th><%= model_class.human_attribute_name(:id) %></th>
      <th><%= model_class.human_attribute_name(:via) %></th>
      <th><%= model_class.human_attribute_name(:status) %></th>
      <th><%= model_class.human_attribute_name(:categoria) %></th>
      <th><%= model_class.human_attribute_name(:data_vencimento) %></th>
      <th><%= model_class.human_attribute_name(:requisicao_id) %></th>
      <th><%= model_class.human_attribute_name(:created_at) %></th>
      <th><%=t '.actions', :default => t("helpers.actions") %></th>
    </tr>
  </thead>
  <tbody>
    <% @carteirinhas.each do |carteirinha| %>
      <tr>
        <td><%= link_to carteirinha.id, carteirinha_path(carteirinha) %></td>
        <td><%= carteirinha.via %></td>
        <td><%= carteirinha.status %></td>
        <td><%= carteirinha.categoria %></td>
        <td><%= carteirinha.data_vencimento %></td>
        <td><%= carteirinha.requisicao_id %></td>
        <td><%=l carteirinha.created_at %></td>
        <td>
          <%= link_to t('.show', :default => t("helpers.links.show")),
                      carteirinha_path(carteirinha), :class => 'btn btn-default btn-xs' %>
          <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
                      carteirinha_path(carteirinha),
                      :method => :delete,
                      :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
                      :class => 'btn btn-xs btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to t('.new', :default => t("helpers.links.new")),
            new_carteirinha_path,
            :class => 'btn btn-primary' %>
